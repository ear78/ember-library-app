<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <td>{{contact.email}}</td>
      <td>{{contact.message}} &nbsp;<button class="btn btn-danger btn-xs" type="button" name="button" {{action 'deleteContact' contact}}>Delete</button></td>
    </tr>
  {{/each}}
  </tbody>
</table>
{{outlet}}
